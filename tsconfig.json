{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "jsx": "react",
  },
  "include": [
    "./**/*",
    
  ],
}
